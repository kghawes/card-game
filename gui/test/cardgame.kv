#:kivy 2.3.1

<Card>:
    size: 200, 300
    canvas:
        Rectangle:
            source: 'card.png'
            pos: self.center_x - self.width / 2, self.center_y - self.height / 2
            size: self.size

<CardGame>:
    card: card

    Card:
        id: card
        center: self.parent.center

